{
  "name": "Small Plushie",
  "description": "You're a small huggable plush size, making you only 1 block tall.",
  "type": "origins:multiple",
  "scale": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "function fluffy-furry-origins:trask/size"
    },
    "interval": 20
  },
  "reset": {
    "type": "origins:action_on_callback",
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "scale reset"
    }
  }
}