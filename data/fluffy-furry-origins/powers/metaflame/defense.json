{
  "name": "Titanium Hull",
  "description": "You have a hard and durable titanium hull giving you better base defence.",
  "type": "origins:multiple",
  "scale": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:defense 2"
